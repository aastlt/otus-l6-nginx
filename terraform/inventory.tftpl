[lb]
balancer ansible_host=${lb_ip}

[apps]
%{ for index, ip in app_ips ~}
wp-app-${index + 1} ansible_host=${ip}
%{ endfor ~}

[db]
wp-db ansible_host=${db_ip}

[private_nodes:children]
apps
db

[private_nodes:vars]
ansible_ssh_common_args='-o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null -o ProxyCommand="ssh -q -o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null -W %h:%p ubuntu@${lb_ip}"'
